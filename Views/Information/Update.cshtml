@{
    ViewData["Title"] = "Update Information";
}

@model Information
<a asp-action="Index" class="btn btn-warning">Go Back</a>
<h1 class="bg-info text-white">Update Information</h1>
<form method="post">
   <div class="form-group">
        <label asp-for="Name">Name:</label>
        <input asp-for="Name" class="form-control" />
    </div class="form-group">
    <div>
        <label asp-for="License">License:</label>
        <input asp-for="License" class="form-control" />
    </div>
    <div>
        <label asp-for="Established">Established:</label>
        <input type=date asp-for="Established" class="form-control" />
    </div>
    <div>
        <label asp-for="Revenue">Revenue:</label>
        <input asp-for="Revenue" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Update</button>
</form>
